<div class="row">
  <div class="col-sm-2"></div>
  <div class="col-sm-6" style="color: white">
    <h1>Lista de Entrenadores</h1>
  </div>
  <div class="col-sm-2">

  </div>
  <div class="col-sm-2"></div>
</div>


<div class="row">
  <div class="col-sm-2"></div>
  <div class="col-sm-6" style="color: white">
    <p>Aqui se listan todos los entrenadores del sistema de pokemon.</p>
  </div>
  <div class="col-sm-2">
    <a href="/CrearEntrenador" class="btn btn-block btn-success">
      Crear Entrenador
    </a>
  </div>
  <div class="col-sm-2"></div>
</div>
<br>


<% for(var i=0; i<entrenadores.length; i++) {%>
<div class="row">
  <div class="col-sm-2"></div>
  <div class="col-sm-8 opaque1">
<div class="row ma-margen-top-bottom">
  <div class="col-sm-1"></div>
  <div class="col-sm-4">
    <div class="text-center ma-borde ma-padding-top-bottom">
      <h1> <%= entrenadores[i].nombres %></h1>
    </div>
  </div>
  <div class="col-sm-1"></div>
  <div class="col-sm-5 text-center">
    <div class="text-center ma-borde" style="margin-top: 40px;">
      <p>
        <%= entrenadores[i].nombres %>
      </p>
      <p>
        <%= entrenadores[i].region %>
      </p>
      <p>
        <%= entrenadores[i].fechaInicio.toDateString() %>
      </p>
    </div>
  </div>
  <div class="col-sm-1">
    <br>
    <a href="/Entrenador/borrarEntrenador?id=<%= entrenadores[i].id %>" class="btn btn-danger btn-block">X</a>
    <a href="/EditarEntrenador?id=<%= entrenadores[i].id %>" class="btn btn-info btn-block">Edit</a>
  </div>
</div>
  </div>
  <div class="col-sm-2"></div>
</div>
<br>
<% }%>










