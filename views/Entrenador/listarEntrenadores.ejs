<div class="row">
  <div class="col-sm-1"></div>
  <div class="col-sm-5" style="color: yellow">
    <h1 align="center">Listado Entrenadores</h1>
  </div>
  <div class="col-sm-6"></div>
</div>


<% for(var i = 0; i < entrenadores.length; i++) { %>
<div class="row">
  <div class="col-sm-1"></div>
  <div class="col-sm-9 opaque1">
    <div class="row ma-margen-top-bottom">
      <div class="col-sm-1"></div>
      <div class="col-sm-4">
        <div class="text-center ma-borde ma-padding-top-bottom">
          <img align="center" id="img1" src="/imagenes/entrenadores/<%= entrenadores[i].imagen %>.png" height="150px">
        </div>
      </div>

      <div class="col-sm-4 text-center">
        <div class="text-center ma-borde" style="margin-top: 40px;">
          <p><%= entrenadores[i].nombres %></p>
          <p><%= entrenadores[i].region %></p>
          <p><%= entrenadores[i].fechaInicio.toDateString() %></p>
        </div>
      </div>
      <div class="col-sm-2">
        <br>
        <br>
        <a href="/Entrenador/borrarEntrenador?id=<%= entrenadores[i].id %>" class="btn btn-danger btn-block">Borrar</a>
        <a href="/EditarEntrenador?id=<%= entrenadores[i].id %>" class="btn btn-info btn-block">Actualizar</a>
        <a href="/CrearPokemon" class="btn btn-block btn-success">Crear Pokemon</a>
      </div>
    </div>
  </div>
  <div class="col-sm-2"></div>
</div>
<br>
<% } %>










